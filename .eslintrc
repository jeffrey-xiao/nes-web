{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "arrow-parens": ["error", "always"],
    "no-bitwise": 0,
    "import/no-unresolved": 0
  },
  "env": {
    "browser": true
  },
  "globals": {
    "__PATH_PREFIX__": false
  }
}
